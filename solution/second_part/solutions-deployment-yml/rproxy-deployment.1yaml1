apiVersion: apps/v1
kind: Deployment
metadata:
  name: reverse-proxy-deployment
  labels:
    app: rproxy
spec:
  replicas: 2
  selector:
    matchLabels:
      app: rproxy      
  template:
    metadata:
      labels:
        app: rproxy
    spec:
      containers:
      - name: rproxy
        image: osmansays/rproxy
        ports:
        - containerPort: 80
        - containerPort: 443
